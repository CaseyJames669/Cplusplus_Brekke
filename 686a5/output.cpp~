#include <fstream>
using namespace std;

// Function: 
// Description: 
// Input: 
// Output: 
//	   
// Preconditions: 
// Postcondtions: 
//			
//


void output(ofstream& out, const int numbers[], int count)
{
  int i;
  //out << "Numbers in array: ";
  for (i = 0; i < count; i++)
    out << numbers[i] << ' ';
  out << endl;
}